{
  "name": "stsp-stack",
  "version": "1.0.0",
  "description": "Stack: source: common: sass, components: modules, sass, js",
  "main": "app.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development --open",
    "start": "webpack --mode development --progress --display-entrypoints",
    "build": "webpack --mode production --progress --display-entrypoints"
  },
  "keywords": [
    "project",
    "standart",
    "package",
    "stack"
  ],
  "author": "stsp",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.10.0",
    "postcss": "^7.0.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "git": "^0.1.5",
    "hamburgers": "^1.1.3",
    "inputmask": "^4.0.5",
    "jquery": "^3.3.1",
    "sublime": "^1.2.1",
    "swiper": "^4.5.0",
    "text": "^0.1.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "no-new": 0,
      "no-shadow": 0,
      "consistent-return": 0,
      "wrap-iife": [
        2,
        "any"
      ],
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ]
    },
    "globals": {
      "document": false,
      "window": true,
      "browser": true
    }
  }
}
